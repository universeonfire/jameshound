<p id="notice"><%= notice %></p>

<h1>Stoks</h1>

<table>
  <thead>
    <tr>
      <th>Stok adi</th>
      <th>Stok adi2</th>
      <th>Stok birim</th>
      <th>Stok gen</th>
      <th>Stok yuk</th>
      <th>Stok der</th>
      <th>Stok Grup</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stoks.each do |stok| %>
      <tr>
        <td><%= stok.stok_adi %></td>
        <td><%= stok.stok_adi2 %></td>
        <td><%= stok.stok_birim %></td>
        <td><%= stok.stok_gen %></td>
        <td><%= stok.stok_yuk %></td>
        <td><%= stok.stok_der %></td>
        <td><%= stok.stok_grup.grup_adi %></td>
        <td><%= link_to 'Show', stok %></td>
        <td><%= link_to 'Edit', edit_stok_path(stok) %></td>
        <td><%= link_to 'Destroy', stok, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stok', new_stok_path %>
